<p id="notice"><%= notice %></p>

<h1>Listing Links</h1>

<table>
  <thead>
    <tr>
      <th>Tinyfied</th>
      <th>Original</th>
      <th>Clicks</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @links.each do |link| %>
      <tr>
        <td><%= link.getTinyUrl %></td>
        <td><%= link.original %></td>
        <td><%= link.clicks %></td>
        <td><%= link_to 'Show', link %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
